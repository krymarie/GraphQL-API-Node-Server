datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id            String   @default(cuid()) @id
  name          String
  content       String
  phone         String
  interestLevel String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}