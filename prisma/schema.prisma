datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:docker@localhost:5432/pg-docker?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id            String   @default(cuid()) @id
  name          String
  status        String
  content       String
  phone         String
  interestLevel String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}